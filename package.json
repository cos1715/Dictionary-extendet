    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-scroll": "^1.5.5",
    "@types/redux-form": "^8.2.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.6.3",
    "react-redux": "^7.1.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "2.1.1",
    "react-scroll": "^1.7.10",
    "react-toastify": "^4.4.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-form": "^8.3.6",
    "redux-saga": "^1.1.3",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "check-formatting": "prettier --list-different \"**/*.{js,json,md,jsx,ts,tsx,css}\"",
    "fix-formatting": "prettier --write \"**/*.{js,json,md,jsx,ts,tsx,css}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss,json,css,md}": [
      "yarn run fix-formatting",
      "git add",
      "yarn run check-formatting"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
